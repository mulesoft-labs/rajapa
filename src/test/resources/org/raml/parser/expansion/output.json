{
    "title": "Example API",
    "version": "v1",
    "traits": {
        "paged": {
            "headers": {
                "page": {
                    "type": "integer"
                }
            }
        },
        "secured": {
            "usage": "Apply this to any method that needs to be secured",
            "description": "Some requests require authentication.",
            "headers": {
                "access_token": {
                    "description": "Access Token",
                    "example": "5757gh76",
                    "required": "true"
                }
            }
        }
    },
    "resources": [
        {
            "relativeUri": "/users",
            "is": ["paged"],
            "methods": [
                {
                    "method": "get",
                    "is": ["secured"],
                    "description": "Some requests require authentication.",
                    "headers": {
                        "page": {
                            "type": "integer"
                        },
                        "access_token": {
                            "description": "Access Token",
                            "example": "5757gh76",
                            "required": "true"
                        }
                    }
                }
            ]
        }
    ]
}
