#%RAML 1.0
title: My API With Types
types:
  Person:
    type: object
    properties:
      name:
        type: string
  Employee:
    type: Person
    properties:
      id:
        type: string
/resource:
  get:
    responses: 
      200:
        body:
          application/json:
            type: Employee
            example:
              name: carlos