#%RAML 1.0
title: My API With Types
types:
  Dog:
    type: object
    properties:
      bite:
       type: string
  Cat:
    type: object
    properties:
      claw:
        type: string
  Parrot:
    type: object
    properties:
      feathers:
        type: string
  HasHome:
    type: object
    properties:
      livesInside:
        type: boolean
  IsOnFarm:
    type: object
    properties:
      livesOutside:
        type: boolean
  HomeAnimal:
    type: [ HasHome | IsOnFarm ,  Dog | Cat | Parrot ]
    properties:
      discriminator:
        type: string
