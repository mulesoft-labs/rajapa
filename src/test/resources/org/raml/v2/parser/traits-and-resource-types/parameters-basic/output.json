{
    "title": "Example API",
    "version": "v1",
    "traits": {
        "secured": {
            "headers": {
                "<<header>>": {
                    "type": "string",
                    "required": "<<req>>"
                }
            }
        }
    },
    "resources": [
        {
            "relativeUri": "/users",
            "displayName": "/users",
            "methods": [
                {
                    "method": "get",
